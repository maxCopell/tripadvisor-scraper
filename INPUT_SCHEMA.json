{
  "title": "TripAdvisor API/Scraper input",
  "description": "To unleash the full power of the scraper/API use the options bellow!",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "locationFullName": {
      "title": "Search",
      "type": "string",
      "description": "Location name as you would enter to TripAdvisor search. Actor takes the first result as desired location. Input countries, cities, neighborhoods",
      "prefill": "Prague",
      "editor": "textfield"
    },
    "maxItems": {
      "title": "Max search results",
      "type": "integer",
      "description": "Limit the number of places to be scraped. Use '0' to not limit the number of places scraped",
      "default": 0,
      "prefill": 10
    },
    "includeAttractions": {
      "title": "Include things to do",
      "type": "boolean",
      "description": "If checked results includes attractions/things to do for given location.",
      "default": true,
      "sectionCaption": "What places to include in the search"
    },
    "includeRestaurants": {
      "title": "Include restaurants",
      "type": "boolean",
      "description": "If checked results includes restaurants for given location.",
      "default": true
    },
    "includeHotels": {
      "title": "Include hotels",
      "type": "boolean",
      "description": "If checked results includes hotels for given location.",
      "default": true
    },
    "checkInDate": {
      "title": "Hotel check-in date (YYYY-MM-DD)",
      "type": "string",
      "default": "",
      "description": "Hotel rooms prices will be calculated for this date",
      "editor": "textfield"
    },
    "locationId": {
      "title": "Location id",
      "type": "string",
      "description": "Id of location. You can use it instead of location name",
      "editor": "textfield",
      "sectionCaption": "Search directly via places Ids",
      "sectionDescription": "You can find the Id of the place in its URL after -d. E.g. for https://www.tripadvisor.cz/Hotel_Review-g186338-d1657415-Reviews-Park_Plaza_Westminster_Bridge_London-London_England.html the id would be 1657415."
    },
    "hotelId": {
      "title": "Hotel id",
      "type": "string",
      "description": "Id of hotel",
      "editor": "textfield"
    },
    "restaurantId": {
      "title": "Restaurant id",
      "type": "string",
      "description": "Id of restaurant",
      "editor": "textfield"
    },
    "includeTags": {
      "title": "Include review tags",
      "description": "Include review tags",
      "default": false,
      "type": "boolean",
      "editor": "checkbox",
      "sectionCaption": "Reviews"
    },
    "includeReviews": {
      "title": "Include reviews",
      "type": "boolean",
      "description": "If checked results includes reviews data",
      "default": false
    },
    "maxReviews": {
      "title": "Limit reviews for each place",
      "type": "integer",
      "description": "Use '0' to not limit the number of reviews scraped",
      "default": 0,
      "prefill": 0
    },
    "lastReviewDate": {
      "title": "Reviews since date (YYYY-MM-DD)",
      "type": "string",
      "description": "It is a date where the scrapper stops loading older reviews ",
      "default": "",
      "editor": "textfield"
    },
