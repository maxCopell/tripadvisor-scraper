{
  "title": "TripAdvisor API/Scrapper input",
  "description": "To unleashed the full power of the scrapper/API use the options bellow!",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "lastReviewDate": {
      "title": "Reviews since date (YYYY-MM-DD)",
      "type": "string",
      "description": "It is a date where the scrapper stops loading older reviews ",
      "default": "",
      "prefill": "2021-01-01",
      "editor": "textfield"
    },
    "locationFullName": {
      "title": "Location",
      "type": "string",
      "description": "Location name as you would enter to TripAdvisor search. Actor takes the first result as desired location",
      "default": "Prague",
      "prefill": "Prague",
      "editor": "textfield"
    },
    "includeRestaurants": {
      "title": "Include restaurants",
      "type": "boolean",
      "description": "If checked results includes restaurants for given location.",
      "default": true,
      "groupCaption": "Options",
      "groupDescription": "Information that will be included"
    },
    "includeAttractions": {
      "title": "Include things to do [BETA]",
      "type": "boolean",
      "description": "If checked results includes hotels for given location.",
      "default": false
    },
    "includeHotels": {
      "title": "Include Hotels",
      "type": "boolean",
      "description": "If checked results includes hotels for given location.",
      "default": true
    },
    "includeReviews": {
      "title": "Include reviews",
      "type": "boolean",
      "description": "If checked results includes reviews data",
      "default": false
    },
    "maxReviews": {
      "title": "Limit reviews for each place",
      "type": "integer",
      "description": "Use '0' to not limit the number of reviews scraped",
      "default": 0
    },
    "maxItems": {
      "title": "Limit the number of places to be scraped",
      "type": "integer",
      "description": "Use '0' to not limit the number of places scraped",
      "default": 0
    },
    "includeTags": {
      "title": "Include restaurant tags",
      "description": "Include restaurant tags",
      "default": false,
      "type": "boolean",
      "editor": "checkbox"
    },
    "locationId": {
      "title": "Location id",
      "type": "string",
      "description": "Id of location",
      "editor": "hidden"
    },
    "hotelId": {
      "title": "Hotel id",
      "type": "string",
      "description": "Id of hotel",
      "editor": "hidden"
    },
    "restaurantId": {
      "title": "Restaurant id",
      "type": "string",
      "description": "Id of restaurant",
      "editor": "hidden"
    },
    "checkInDate": {
      "title": "Check in date (YYYY-MM-DD)",
      "type": "string",
      "default": "",
      "description": "Hotel rooms prices will be calculated for this date",
      "editor": "textfield"
    },
